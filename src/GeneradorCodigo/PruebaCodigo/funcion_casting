prueba BEGIN

TIPO_UNSIGNED FUN funcion1 ( TIPO_UNSIGNED x )
	    BEGIN
	        TIPO_UNSIGNED z;
	        z:= 50;
	        IF ( x > 45)  THEN
            BEGIN
                RET (x);
            END
            END_IF;
	        RET (x+z);
	    END;

TIPO_SINGLE x;
x:= 1.1s+25;

funcion1(TIPO_UNSIGNED(x));


END;