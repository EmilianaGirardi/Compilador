prueba BEGIN

	TIPO_UNSIGNED FUN funcion1 ( TIPO_UNSIGNED x )
		    BEGIN
		       
		        RET (10+x);
		    END;
	
	TIPO_UNSIGNED x;
	x:= 1;
	
	TIPO_UNSIGNED y;
	
	y:=funcion1(x);
	
	OUTF([FUERA de funcion1]);
	
	IF(y<50)THEN
		BEGIN
			OUTF([y menor a 50 => el RET fue x]);
		END
	ELSE
		BEGIN
			OUTF([y mayor a 50 => el RET fue x+z]);
		END
	END_IF;

END;