prueba BEGIN

TIPO_UNSIGNED FUN funcion1 ( TIPO_UNSIGNED x )
	    BEGIN
	        TIPO_UNSIGNED z;
	        z:= 50;
	        IF ( x > 45)  THEN
	            BEGIN
	            	OUTF([x es mayor a 45]);
	                RET (x);
	            END
            ELSE
            	BEGIN
            		OUTF([x es menor a 45]);
            	END
            END_IF;
	        RET (x+z);
	    END;

TIPO_SINGLE x;
x:= 1.1s+25;

funcion1(TIPO_UNSIGNED(x));
OUTF([FUERA de funcion1]);

END;