prueba BEGIN

TIPO_UNSIGNED x, y, z, i, j , k;

x:= 5;
y:= 48;
z:= 59;
i:= 84;
j:= 95;
k:= 105;

IF (z < 80) THEN BEGIN
    REPEAT BEGIN
        IF(y <= 26) THEN BEGIN
            GOTO salir@;
        END
        END_IF;
    END
    UNTIL(x > 25);
END
END_IF;

TIPO_OCTAL var;
var:= 05;

x:= x+25;

salir@;

x:= 10;

END;