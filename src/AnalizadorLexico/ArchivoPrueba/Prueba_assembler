prueba BEGIN
    TIPO_UNSIGNED x, v;
    TIPO_SINGLE y, z;
	
    x := x + v;
    y := 12.2s+8;
    y := y - z;

    TIPO_UNSIGNED FUN funcion1 (TIPO_SINGLE x )
	    BEGIN
	        IF ( x = 1.0)  THEN
	            BEGIN
	                RET (1);
	            END
	        END_IF;
	
	        RET (123);
	    END;
	    
	TIPO_SINGLE g;
	funcion1(g);

END;