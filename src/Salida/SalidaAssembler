.MODEL flat, stdcall
option casemap :none
include \masm32\include\masm32rt.inc
includelib \masm32\lib\kernel32.lib
includelib \masm32\lib\masm32.lib
.STACK 200h

.DATA
z.global DD ? 
v.global DW ? 
_float0_ DD 12.2e+8
x.global DW ? 
y.global DD ? 

.CODE
START:
MOV AX, x.global
ADD AX, v.global
MOV @aux1, AX
MOV AX, @aux1
MOV x.global, AX
MOV ST, _float0_
MOV y.global, ST
MOV ST, y.global
FSUB ST, z.global
MOV @aux2, ST
MOV ST, @aux2
MOV y.global, ST
END START